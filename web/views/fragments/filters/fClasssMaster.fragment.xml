<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog title="Select Filter">
		<List items="{/classMasterFilterData}" mode="SingleSelectLeft" includeItemInSelection="true" select="onGetFilterData">
			<StandardListItem title="{value}" iconInset="true"/>
		</List>
		<buttons>
			<Button id="confirmClassSaveBtn" text="Confirm" visible="false" press="onPressSelectFilterName"/>
			<Button text="Close" press="onPressFilterClose"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>